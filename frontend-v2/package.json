{
  "name": "echoic-space",
  "version": "2.0.0",
  "description": "Website: Diego Villaseñor, músico, compositor, programador",
  "scripts": {
    "node-repl": "shadow-cljs cljs-repl app",
    "browser-repl": "shadow-cljs cljs-repl browser",
    "release": "run-s release:*",
    "release:node": "shadow-cljs release app",
    "watch:node": "shadow-cljs watch app",
    "ssr": "shadow-cljs release ssr && node build/ssr.js",
    "release:browser": "shadow-cljs release browser",
    "watch:browser": "shadow-cljs watch browser",
    "start:browser": "serve -s build/browser",
    "build:content": "node node_modules/frontmatter-markdown-to-json --in=build/browser/ --out=build/browser/data",
    "build": "npm run build:content && npm run build:sass && npm run ssr && npm run release:browser",
    "prefix-css": "postcss --use autoprefixer cssnano -b 'last 5 versions' build/browser/css/mazorca.css -o build/browser/css/mazorca.css",
    "watch:sass": "sass src/sass/mazorca.scss build/browser/css/mazorca.css --watch",
    "build:sass": "sass src/sass/mazorca.scss build/browser/css/mazorca.css && npm run prefix-css"
  },
  "keywords": [],
  "devDependencies": {
    "autoprefixer": "^10.2.1",
    "cssnano": "^5.0.2",
    "frontmatter-markdown-to-json": "^0.0.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.3",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.2",
    "serve": "^11.1.0",
    "shadow-cljs": "^2.8.39"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "howler": "2.1.3",
    "mazorca-core": "^1.6.2",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  }
}
