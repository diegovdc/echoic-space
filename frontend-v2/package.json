{
  "name": "echoic-space",
  "version": "2.0.0",
  "description": "Website: Diego Villaseñor, músico, compositor, programador",
  "scripts": {
    "node-repl": "shadow-cljs cljs-repl app",
    "browser-repl": "shadow-cljs cljs-repl browser",
    "release": "run-s release:*",
    "release:node": "shadow-cljs release app",
    "watch:node": "shadow-cljs watch app",
    "ssr": "shadow-cljs release ssr && node build/ssr.js",
    "release:browser": "shadow-cljs release browser",
    "watch:browser": "shadow-cljs watch browser",
    "start:browser": "serve -s build/browser",
    "build:content": "node node_modules/frontmatter-markdown-to-json --in=build/browser/ --out=build/browser/data",
    "build": "npm run build:content && npm run ssr && npm run release:browser"
  },
  "keywords": [],
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "serve": "^11.1.0",
    "frontmatter-markdown-to-json": "0.0.10",
    "shadow-cljs": "^2.8.39"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "howler": "2.1.3",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  }
}
